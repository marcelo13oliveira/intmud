  se arg1 == "Desejo morar aqui"
    ret arg0.var.z_salafim == txt(dono), arg0.msg("O guardião do acampamento diz 'Mas você já \
mora aqui.")
    ret arg0.var.z_moedas_ < 50, arg0.msg("O guardião do acampamento diz 'Para morar conosco \
você deve contribuir com 50 moedas, a quantia que você não tem. vá conseguir!")
    arg0.var.z_moedas_ -= 50
    arg0.var.z_salafim = dono
    $mens.p(este, arg0)
    $mens.mtodos1("O guardião recolhe sua contribuição e aceita-lhe como mais novo morador do \
acampamento.")


