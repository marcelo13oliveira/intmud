func cmd_atacar
  se txt(arg2) == "cmd_rasteira"
    $mens.p(arg0, arg1)
    $mens.mtodos1("Quando $p tenta derrubar $a com uma rasteira, el$w salta, esquivando-se.")
    ret 1
  fimse
  se txt(arg2) == "cmd_derrubar"
    $mens.p(arg0, arg1)
    $mens.mtodos1("Quando $p tenta derrubar $a com um soco, el$w salta, esquivando-se.")
    ret 1
  fimse
  se txt(arg2) == "cmd_desarmar"
    $mens.p(arg0, arg1)
    $mens.mvis1("$P não consegue desarmar $A.")
    ret 1

