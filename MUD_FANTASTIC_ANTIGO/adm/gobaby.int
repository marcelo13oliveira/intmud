func escr
  textotxt t
  listaobj l
  epara l.addfim(arg0.dono.dentro2), l, l.ini.remove
    se txt(l.objini) == "p_templo012"
      l.objini.mudadono($s_rio150)
      t.addfim(l.objini.descnome)
    fimse
  efim
  se t.linhas
    $mens.p(arg0)
    $mens.mtodos1("Foram mandados para a creche: " + txttroca(t.remove(1000), "\n", ", "))
  senao
    arg0.msg("Sem crianças para mandar para a creche.")
  fimse
